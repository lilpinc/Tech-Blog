<h2 class="">Log In</h2>
  <form id="" class="">
    <div class="username-login">
      <label class="form-label" for="username-login">Username</label>
      <input  id="username" type="text" class="form-input"/>
    </div>
    <div class="password-login">
      <label class="form-label" for="password-login">Password</label>
      <input id="password-login" type="password" class="form-input"/>
    </div>
      <button type="submit" class="btn">Login</button>
      <a class="btn login-link" href="/signup">Sign Up</a>
  </form>
    
<script src="/js/login.js"></script>